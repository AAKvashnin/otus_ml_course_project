name: ComplaintClassifier

conda_env: conda.yaml

entry_points:
  # train Spark model
  main:
    parameters:
       max_iter: {type: int, default: 10}
       reg_param: {type: float, default: 0.3}
    command: "python mlflow/train.py  {max_iter} {reg_param}"
  register:
    command: "python mlflow/register.py"
  unregister:
    command: "python mlflow/unregister.py"
